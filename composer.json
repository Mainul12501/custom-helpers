{
    "name": "mainul/custom-helpers",
    "description": "A Laravel package providing helper functions for API and web responses",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "helper",
        "api",
        "response"
    ],
    "authors": [
        {
            "name": "Mainul Islam",
            "email": "mainul12501@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/routing": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Mainul\\CustomHelperFunctions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mainul\\CustomHelperFunctions\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mainul\\CustomHelperFunctions\\HelperServiceProvider"
            ],
            "aliases": {
                "ViewHelper": "Mainul\\CustomHelperFunctions\\Facades\\ViewHelper"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
